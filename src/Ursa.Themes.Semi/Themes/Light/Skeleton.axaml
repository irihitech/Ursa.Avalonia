    <LinearGradientBrush x:Key="SkeletonStartAnimationBackground" StartPoint="0%,100%" EndPoint="100%,100%">
        <GradientStop Offset="0" Color="#F9F9F9" />
        <GradientStop Offset="0" Color="#E6E8EA" />
        <GradientStop Offset="1" Color="#F9F9F9" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="SkeletonEndAnimationBackground" StartPoint="0%,100%" EndPoint="100%,100%">
        <GradientStop Offset="0" Color="#F9F9F9" />
        <GradientStop Offset="1" Color="#E6E8EA" />
        <GradientStop Offset="1" Color="#F9F9F9" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="SkeletonDefaultBackground" Color="#F9F9F9" />
</ResourceDictionary>